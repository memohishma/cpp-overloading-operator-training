1.
#include<iostream>
using namespace std;
//(*=,/=,+=,-=,%=)
int main()
{
    int x=3;
    int y=4;
    int z=5;
    int c=3;
    x*=2;
    cout<<x<<endl;//6
    y+=2;
    cout<<y<<endl;//6
    z%=10;
    cout<<z<<endl;//5
    c/=1;
    cout<<c<<endl;//3
    
    return 0;
}
